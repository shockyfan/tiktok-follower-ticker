void setup() {

  Serial.begin(115200);
  WiFi.begin(ssid, password);
  Serial.println("Connecting");

  display.init();
  display.flipScreenVertically();
  display.setFont(ArialMT_Plain_10);
  display.clear();
  display.setTextAlignment(TEXT_ALIGN_CENTER);
  display.setFont(ArialMT_Plain_16);
  display.drawString(63, 0, "Conecting to Wifi");
  display.drawXbm(34, 20, WiFi_Logo_width, WiFi_Logo_height, WiFi_Logo_bits);
  display.display();
    
  while (WiFi.status() != WL_CONNECTED) {
    delay(500);
    Serial.print(".");
  }
  Serial.println("");
  Serial.print("Connected to WiFi network with IP Address: ");

  display.clear();
  display.setTextAlignment(TEXT_ALIGN_CENTER);
  display.setFont(ArialMT_Plain_16);
  display.drawString(63, 0, "Connected");
  display.drawXbm(34, 20, WiFi_Logo_width, WiFi_Logo_height, WiFi_Logo_bits);
  display.display();
  delay(3000);
  display.clear();
  display.setTextAlignment(TEXT_ALIGN_CENTER);
  display.setFont(ArialMT_Plain_16);
  display.drawString(63, 0, "TikTokFollower");
  Serial.println(WiFi.localIP());
  followerCount = httpGETRequest(serverName);
  altfollower = followerCount.toInt();  
  updateFollowerCount();
}

void updateOLED() {  
  display.clear();
  display.setTextAlignment(TEXT_ALIGN_CENTER);
  display.setFont(ArialMT_Plain_16);
  display.drawString(63, 0, "TikTokFollower");
  display.setFont(ArialMT_Plain_16);
  display.drawString(63, 20, followerCount);
  display.display();
}

void updateFollowerCount() {
  if (WiFi.status() == WL_CONNECTED) {
    followerCount = httpGETRequest(serverName);
    Serial.println(followerCount);
    akutellefollower = followerCount.toInt();
    if (altfollower < akutellefollower ){
      animation();
      altfollower = akutellefollower;
    }
    updateOLED();

  }
  else {
    Serial.println("WiFi Disconnected");
  }
  lastTime = millis();

}

